{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"dynamodb:GetItem",
				"dynamodb:PutItem",
				"dynamodb:UpdateItem",
				"dynamodb:DeleteItem",
				"dynamodb:Query",
				"dynamodb:Scan"
			],
			"Resource": [
				"arn:aws:dynamodb:ap-south-1:732029699394:table/Events",
				"arn:aws:dynamodb:ap-south-1:732029699394:table/Events/index/*",
				"arn:aws:dynamodb:ap-south-1:732029699394:table/Semesters"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogGroup",
				"logs:CreateLogStream",
				"logs:PutLogEvents"
			],
			"Resource": "*"
		}
	]
}
